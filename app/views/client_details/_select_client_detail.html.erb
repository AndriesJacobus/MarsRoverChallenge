
<div class = "z-depth-5 hoverable scroller">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Company name</th>
          <th>Business address</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @client_details.each do |client_detail| %>
          <tr>
            <td><%= client_detail.name %></td>
            <td><%= client_detail.company_name %></td>
            <td><%= client_detail.business_address %></td>
            <td><%= link_to 'Link Client', "", class: "waves-effect waves-light blue lighten-1 btn-small white-text disabled" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>

