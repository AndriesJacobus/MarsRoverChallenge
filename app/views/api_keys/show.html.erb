<p id="notice"><%= notice %></p>

<br/>

<div class="row">
  <div class="col s12 m6">
    <div class="card blue white-text z-depth-5 hoverable">
      <div class="card-content">
        <span class="card-title">API Key Information:</span>
        <hr style = "display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0;"/>

        <p>
          <strong>Username:</strong>
          <br/>
          <%= @api_key.username %>
        </p>

        <br/>

        <p>
          <strong>Password:</strong>
          <br/>
          <%= @api_key.password %>
        </p>

        <br/>
      </div>
    </div>
  </div>
</div>

<br/>

<%= link_to 'Edit', edit_api_key_path(@api_key), class: "waves-effect waves-light blue lighten-1 btn-small white-text" %> |
<%= link_to 'Back', api_keys_path, class: "waves-effect waves-light blue lighten-1 btn-small white-text" %>
